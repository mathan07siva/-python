   "source": [
    "# using while loop\n",
    "\n",
    "num=int(input('enter the no:'))\n",
    "fact=1\n",
    "while(num>0):\n",
    "    fact=fact * num\n",
    "    num = num-1\n",
    "print(fact)"
   ]
  }
